<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

        <HBox xmlns="http://javafx.com/javafx"
        xmlns:fx="http://javafx.com/fxml"
        fx:controller="ControllerClassesExample.ControllerOne"
        prefHeight="200.0" prefWidth="300.0">

        <Button fx:id="helloButton" mnemonicParsing="false" onAction="#printHello" text="Hello" />

        <Button fx:id="worldButton" mnemonicParsing="false" onAction="#printWorld" text="World" />

        <Label fx:id="labelOne" text="Initial Label Text" />
        </HBox>